ion === 'decrement') {\r\n               setQty(qty - 1) \r\n          }\r\n        }\r\n\r\n        const remove = (product:Product) => {\r\n          dispatch(removeFromCart(product))\r\n        }\r\n\r\n        useEffect( () => {\r\n            updateBag(props.Item, qty)\r\n          },[qty])\r\n\r\n\r\n    return (\r\n        <tr key={props.key} >\r\n        <td>\r\n            <img\r\n            width=\"70\"\r\n            height=\"70\"\r\n            src={imgPath}\r\n            alt=\"...\"\r\n            />\r\n        </td>\r\n        <td>{props.Item.title}</td>\r\n        <td>€{props.Item.price}</td>\r\n        <td>\r\n            <div className=\"btn-group\" role=\"group\" aria-label=\"Basic example\">\r\n            <button\r\n                type=\"button\"\r\n                className=\"btn btn-secondary\"\r\n                onClick={() => {\r\n                if (qty > 1) {update(props.Item, 'decrement') }\r\n                }}\r\n                >\r\n                -\r\n            </button>\r\n            <span className=\"btn btn-light\">{qty}</span>\r\n            <button\r\n                type=\"button\"\r\n                className=\"btn btn-secondary\"\r\n                onClick={() =>  {update(props.Item, 'increment')}}\r\n                >\r\n                +\r\n            </button>\r\n            </div>\r\n        </td>\r\n        <td>{(props.Item.quantity *  props.Item.price).toFixed(2)}€ </td>\r\n        <td>\r\n            <button\r\n            type=\"button\"\r\n            className=\"btn btn-danger remove\"\r\n                onClick={() => remove(props.Item)}>\r\n            X\r\n            </button>\r\n        </td>\r\n        </tr>\r\n    );\r\n}\r\n    \r\n  \r\nexport default CartRow;","c:\\GIthub\\fabricsaddict\\front\\src\\Components\\Card\\Card.tsx",["269","270","271","272","273","274","275","276","277","278","279","280","281","282","283"],"import React, { ReactNode, useRef,useState, FunctionComponent } from \"react\";\r\n